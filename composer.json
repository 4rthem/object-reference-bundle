{
    "name": "arthem/object-reference-bundle",
    "description": "Object reference bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "7 || ^8",
        "symfony/framework-bundle": "^6.3 || ^7",
        "doctrine/orm": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1.3",
        "friendsofphp/php-cs-fixer": "^v3.58.1",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Arthem\\ObjectReferenceBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alchemy\\Bundle\\ObjectReferenceBundle\\Tests\\": "/Tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "vendor/bin/php-cs-fixer fix"
    }
}
